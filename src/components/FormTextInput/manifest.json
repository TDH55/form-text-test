{
  "displayName": "form text input",
  "defaultWidth": 160,
  "defaultHeight": 24,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "text",
      "displayName": "Text",
      "type": "text",
      "default": "Happy Hacking"
    },
    {
      "name": "color",
      "displayName": "Color",
      "type": "color",
      "default": "#00A898"
    },
    {
      "name": "formComponentInput",
      "displayName": "input",
      "type": "text",
      "role": "formValue"
    },
    {
      "name": "secondFormComponentInput",
      "displayName": "Second input",
      "type": "text",
      "role": "formValue"
    },
    {
      "name": "controlledValue",
      "displayName": "What Property Does This Adjust?",
      "type": "number",
      "role": "autosaveInput"
    },
    {
      "name": "exampleAction",
      "displayName": "Example Action",
      "type": "action",
      "arguments": [
        {
          "type": "number",
          "displayName": "Argument 1"
        },
        {
          "type": "text",
          "displayName": "Argument 2"
        }
      ]
    }
  ],
  "childComponents": [
    {
      "name": "anotherInput",
      "displayName": "Another Input",
      "props": [
        {
          "name": "anotherInputValue",
          "displayName": "Another Input Value",
          "type": "text",
          "role": "formValue"
        },
        {
          "name": "anotherAutoSave",
          "displayName": "Another Auto Save",
          "type": "number",
          "role": "autosaveInput"
        }
      ]
    }
  ]
}